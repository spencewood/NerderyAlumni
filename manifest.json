{
  "background": {
      "page": "background.html"
  },
"manifest_version": 2,

  "name": "Nerdery Alumni",
  "description": "Who is leaving? Who was asked to leave?",
  "version": "1.0",

  "permissions": [
    "https://mainframe.nerdery.com/",
    "alarms"
  ], 
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [ 
      "alumniHttp.js",
      "background.js"
  ],
  "icons": { 
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png" 
  }
}